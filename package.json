  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "start": "web-ext run --bc --url 'about:addons' -s dist/",
    "exec": "yarn run build && yarn run start",
    "build-ext": "yarn run build && web-ext build -s dist/ -a artifacts/",
    "inspect": "webpack --profile --json > report.json; webpack-bundle-analyzer report.json --mode static && sleep 6 && rm report.{json,html}",
    "update": "ncu --upgrade --upgradeAll && yarn upgrade --latest"
  },
  "dependencies": {
    "@material/checkbox": "^0.3.7",
    "@material/select": "^0.3.9",
    "@material/switch": "^0.1.9",
    "lodash": "^4.17.4",
    "spinkit": "^1.2.5",
    "uuid": "^3.1.0",
    "vue": "^2.3.4",
    "vuedraggable": "^2.14.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.5.2",
    "babili-webpack-plugin": "^0.1.2",
    "commander": "^2.10.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.4.2",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-svgmin": "^1.2.4",
    "lodash-webpack-plugin": "^0.11.4",
    "npm-check-updates": "^2.12.1",
    "sass-loader": "^6.0.6",
    "shortid": "^2.2.8",
    "vue-loader": "^12.2.1",
    "vue-template-compiler": "^2.3.4",
    "web-ext": "^1.10.0",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  }
}
